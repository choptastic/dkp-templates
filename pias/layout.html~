	<script language=javascript>
		function menuwidth()
		{
			var curw = document.body.clientWidth;
			var t = document.getElementById("topmenu").tBodies[0].rows[0].cells;
			var numitems = t.length;
			var newwidth = Math.floor(curw/numitems) + "px";
			var i;
			for(i=0;i<numitems;i++)
				t[i].style.width=newwidth;
		}
	</script>
</head>
